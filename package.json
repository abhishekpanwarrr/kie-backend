{
  "name": "kie-backend",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "prisma": {
    "seed": "bun prisma/seed.ts"
  },
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "jest --watch",
    "migrate": "node scripts/migrate.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "lint": "eslint src/**/*.js",
    "prisma:migrate": "dotenv -e .env.prisma -- prisma migrate dev",
    "prisma:deploy": "dotenv -e .env.prisma -- prisma migrate deploy",
    "prisma:generate": "prisma generate"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/bun": "latest",
    "@types/node": "^25.0.6",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "eslint": "^8.56.0",
    "prisma": "^5.9.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "aws-sdk": "^2.1693.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^5.0.0",
    "express-rate-limit": "^7.3.0",
    "express-validator": "^7.3.1",
    "helmet": "^7.1.0",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "redis": "^5.10.0",
    "resend": "^6.7.0",
    "socket.io": "^4.7.5",
    "stripe": "^20.1.2",
    "winston": "^3.19.0",
    "zod": "^4.3.4"
  }
}
